.list-tip{
    display: flex;
    flex-direction: row;
    width:94%;
    padding:2% 3%;
    color:#999;
    font-size: 28rpx;
    background-color: #f5f5f5;
}
.tip-circle{
    display: inline-block;
    margin:2rpx 10rpx;
    width:12rpx;
    height: 40rpx;
    border-radius:6rpx;
    background-color: #3f88e1; 
}